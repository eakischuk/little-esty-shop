<h3>Create New Bulk Discount</h3>
<%= form_with model: @discount, url: merchant_bulk_discounts_path(@merchant, @discount), method: :post, local: true do |f| %>
  <%= f.label :percentage %>
  <%= f.number_field :percentage %>
  <%= f.label :quantity_threshold %>
  <%= f.number_field :quantity_threshold %>
  <%= f.submit "Create Bulk Discount" %>
<% end %>
